{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        
    ],
    "parser":"babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "security"
    ],
    "rules": {
        "react/prop-types": "off",
        "security/detect-non-literal-fs-filename": "off"
    }
}